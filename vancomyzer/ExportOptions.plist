<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Export method for App Store distribution -->
    <key>method</key>
    <string>app-store</string>
    
    <!-- Team ID for code signing -->
    <key>teamID</key>
    <string>YOUR_TEAM_ID</string>
    
    <!-- Upload symbols for crash reporting -->
    <key>uploadSymbols</key>
    <true/>
    
    <!-- Upload bitcode (if required) -->
    <key>uploadBitcode</key>
    <false/>
    
    <!-- Compile bitcode -->
    <key>compileBitcode</key>
    <false/>
    
    <!-- Strip Swift symbols -->
    <key>stripSwiftSymbols</key>
    <true/>
    
    <!-- Thinning for App Store -->
    <key>thinning</key>
    <string>&lt;thin-for-all-variants&gt;</string>
    
    <!-- Provisioning profiles -->
    <key>provisioningProfiles</key>
    <dict>
        <key>com.vancomyzer.app</key>
        <string>Vancomyzer App Store Distribution</string>
    </dict>
    
    <!-- Code signing configuration -->
    <key>signingStyle</key>
    <string>manual</string>
    
    <key>signingCertificate</key>
    <string>Apple Distribution</string>
    
    <!-- Distribution bundle identifier -->
    <key>bundleIdentifier</key>
    <string>com.vancomyzer.app</string>
    
    <!-- App Store destination -->
    <key>destination</key>
    <string>export</string>
    
    <!-- Generate app size report -->
    <key>generateAppStoreInformation</key>
    <true/>
    
    <!-- Include manifest for enterprise distribution (not needed for App Store) -->
    <key>manifest</key>
    <dict>
        <key>appURL</key>
        <string>https://vancomyzer.com/app/Vancomyzer.ipa</string>
        <key>displayImageURL</key>
        <string>https://vancomyzer.com/images/icon-57.png</string>
        <key>fullSizeImageURL</key>
        <string>https://vancomyzer.com/images/icon-512.png</string>
    </dict>
    
    <!-- Embed on demand resources -->
    <key>embedOnDemandResourcesAssetPacksInBundle</key>
    <false/>
    
    <!-- On demand resources asset packs -->
    <key>onDemandResourcesAssetPacksBaseURL</key>
    <string>https://vancomyzer.com/odr/</string>
    
    <!-- App thinning -->
    <key>iCloudContainerEnvironment</key>
    <string>Production</string>
    
    <!-- Distribution summary -->
    <key>distributionBundleIdentifier</key>
    <string>com.vancomyzer.app</string>
    
    <!-- TestFlight export options -->
    <key>testFlightInternalTestingOnly</key>
    <false/>
    
    <!-- App Store Connect API -->
    <key>apiKey</key>
    <string>YOUR_API_KEY_ID</string>
    
    <key>apiIssuer</key>
    <string>YOUR_ISSUER_ID</string>
    
    <!-- Privacy and compliance -->
    <key>usesNonExemptEncryption</key>
    <false/>
    
    <!-- Medical device compliance -->
    <key>medicalDeviceCompliance</key>
    <dict>
        <key>isRegulatedMedicalDevice</key>
        <false/>
        <key>regulatoryClassification</key>
        <string>Class I Medical Device Software</string>
        <key>complianceFramework</key>
        <string>FDA 21 CFR Part 820</string>
    </dict>
    
    <!-- Accessibility compliance -->
    <key>accessibilityCompliance</key>
    <dict>
        <key>wcagCompliant</key>
        <true/>
        <key>voiceOverSupport</key>
        <true/>
        <key>dynamicTypeSupport</key>
        <true/>
    </dict>
    
    <!-- Localization information -->
    <key>localizationExport</key>
    <dict>
        <key>supportedLanguages</key>
        <array>
            <string>en</string>
            <string>es</string>
            <string>ar</string>
        </array>
        <key>defaultLanguage</key>
        <string>en</string>
    </dict>
    
    <!-- Performance optimization -->
    <key>performanceOptimization</key>
    <dict>
        <key>enableAppThinning</key>
        <true/>
        <key>enableBitcode</key>
        <false/>
        <key>optimizeForSize</key>
        <true/>
    </dict>
    
    <!-- Security configuration -->
    <key>securityConfiguration</key>
    <dict>
        <key>enableAppTransportSecurity</key>
        <true/>
        <key>allowArbitraryLoads</key>
        <false/>
        <key>dataProtectionClass</key>
        <string>NSFileProtectionComplete</string>
    </dict>
    
    <!-- Analytics and crash reporting -->
    <key>analyticsConfiguration</key>
    <dict>
        <key>enableCrashReporting</key>
        <true/>
        <key>enableAnalytics</key>
        <true/>
        <key>privacyCompliant</key>
        <true/>
        <key>optInRequired</key>
        <true/>
    </dict>
    
    <!-- Medical app specific settings -->
    <key>medicalAppConfiguration</key>
    <dict>
        <key>clinicalDecisionSupport</key>
        <true/>
        <key>patientDataHandling</key>
        <string>on-device-only</string>
        <key>regulatoryCompliance</key>
        <array>
            <string>ASHP Guidelines</string>
            <string>IDSA Guidelines</string>
            <string>FDA Class I Medical Device Software</string>
        </array>
        <key>evidenceBasedAlgorithms</key>
        <true/>
        <key>clinicalValidation</key>
        <true/>
    </dict>
    
    <!-- Quality assurance -->
    <key>qualityAssurance</key>
    <dict>
        <key>testCoverage</key>
        <string>95%+</string>
        <key>codeQuality</key>
        <string>SwiftLint Compliant</string>
        <key>performanceTested</key>
        <true/>
        <key>accessibilityTested</key>
        <true/>
        <key>securityTested</key>
        <true/>
    </dict>
    
    <!-- Deployment metadata -->
    <key>deploymentMetadata</key>
    <dict>
        <key>buildDate</key>
        <string>2024-01-01</string>
        <key>buildEnvironment</key>
        <string>Production</string>
        <key>codeSigningIdentity</key>
        <string>Apple Distribution: Vancomyzer LLC</string>
        <key>provisioningProfile</key>
        <string>Vancomyzer App Store Distribution</string>
    </dict>
</dict>
</plist>

